<h1><%=@user.name%> </h1>

<h3> Liked Songs </h3> 
<% @songs.each do |song| %>
<%= song.name %>
<%= song.audio_file %><br>
<% end %>

<h3> Followed Artists </h3>
<% @artists.each do |artist| %>
<%= link_to artist.name, artist_path(artist) %>
<% end %>


<%= button_to "Delete", user_path(@user), method: :delete %>
<%= button_to "Edit", edit_user_path(@user), method: :get %>
